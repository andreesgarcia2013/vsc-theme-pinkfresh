	"tokenColors": [
		{
			"scope": ["comment"],
			"settings": {
			  "foreground": "#777688",
			  "fontStyle": "italic"
			}
		  },
		  {
			"scope": ["constant"],
			"settings": {
			  "foreground": "#ffffff"
			}
		  },
		  {
			"scope": [
			  "constant.numeric",
			  "constant.language",
			  "constant.charcter.escape"
